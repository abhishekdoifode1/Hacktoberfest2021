#include<stdio.h>
int main()
{
	int secretno = 5;
	int guess;
	int guesscount = 0 ;
	int guesslimit = 3 ;
	int  outofguesses = 0;

	while( guess != secretno && outofguesses == 0){
		if(guesscount < guesslimit){
		 printf("Enter Number: ");
		 scanf("%d", &guess);		
		 guesscount++ ;
	}else {
		outofguesses = 1;
		
			
	}
	}
	  
	  if  (outofguesses == 1){
	  	printf("Out of Guesses");
	  }else {
	  	printf("You Win!");
	  }
	return 0;
}
